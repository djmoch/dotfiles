if(~ $#DISPLAY 0) {
	if(which lynx >/dev/null >[2=1]) {
		BROWSER=lynx
	}
	if(which w3m >/dev/null >[2=1]) {
		BROWSER=w3m
		WWW_HOME=https://www.danielmoch.com
	}
}
if not {
	if(which firefox >/dev/null >[2=1]) {
		BROWSER=firefox
	}
	if(which firefox-esr >/dev/null >[2=1]) {
		BROWSER=firefox-esr
	}
	if(which surf >/dev/null >[2=1]) {
		BROWSER=surf
	}
}
