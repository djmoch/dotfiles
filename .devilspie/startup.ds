(if (is (application_name) "Xfce Terminal")
    (begin
        (set_workspace 2)
        (skip_pager)
        (fullscreen)
    )
)

(if
    (not 
        (or
            (is (application_name) "Xfce Terminal")
            (is (application_name) "pinentry")
            (is (window_name) "Whisker Menu")
            (is (application_name) "xfce4-panel")
        )
    )
    (begin
        (set_workspace 1)
    )
)
