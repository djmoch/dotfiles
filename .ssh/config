Host dotcom-git
    HostName git.danielmoch.com

Host dotorg-git
    HostName git.djmoch.org

Host *-git
    User git
    ForwardAgent no

Host sweetwater* sw*
    HostName sweetwater.djmoch.org

Host longboard* lb*
    HostName longboard.djmoch.org

Host urbock-mgmt*
    HostName urbock-mgmt.djmoch.org
    CheckHostIP no
    User root

Host urbock* ub*
    HostName urbock.djmoch.org

Host hoplift* hl* dotcom dotcom-gpg dc*
    HostName hoplift.danielmoch.com

Host webhost* wh* webhost.djmoch.org
    HostName webhost.djmoch.org

Host unifi uf
    HostName urbock-mgmt.djmoch.org
    HostKeyAlias unifi.djmoch.org
    CheckHostIP no
    User root

Host *-gpg
    RemoteForward /run/user/10000/gnupg/S.gpg-agent /run/user/10000/gnupg/S.gpg-agent.extra

Host github github.com
    HostName github.com
    User git
    ForwardAgent no

Host *
    User djmoch
    ForwardAgent yes
